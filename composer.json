{
    "name": "motia/laravel-translations-port",
    "description": "Import/Export your laravel translations(including the missing ones!) to/from your separate frontend apps.",
    "keywords": ["translations", "i18n", "spa", "import", "export", "translations manager", "missing translations", "laravel", "vue-i18n"],
    "license": "MIT",
    "require": {},
    "require-dev": {
        "vsch/laravel-translation-manager": "^2.6",
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Motia\\TranslationsPort\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motia\\TranslationsPort\\TranslationsPortServiceProvider"
            ]
        }
    },
    "suggest": {
        "vsch/laravel-translation-manager": "Required to the translation manager UI and use VschTranslationsLoader"
    }
}
